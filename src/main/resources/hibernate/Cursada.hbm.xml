<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Cursada" table="CURSADA">
		<meta attribute="class-description">
			Esta clase contiene la informacion de las cursadas
		</meta>
		<id name="idCursada" type="int" column="id_Cursada">
			<generator class="native"/>
		</id>
		<property name="inicio" column="inicio" type="date"/>
		<list name="pruebas" cascade="all">
			<key column="idPrueba"/>
			<index column="idPrueba"/>
			<one-to-many class="Prueba"/>
		</list>
		<many-to-one name="curso" class="Curso"
			cascade="all"></many-to-many>
		<many-to-many name="usuario" class="Usuario"
			cascade="all"></many-to-many>
	</class>

</hibernate-mapping>